    "chai": "^4.3.6",
    "easymde": "^2.16.1",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.4",
    "hardhat": "^2.9.3",
    "ipfs-http-client": "^56.0.3",
    "next": "12.1.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-markdown": "^8.0.3",
    "react-simplemde-editor": "^5.0.2",
    "web3modal": "^1.9.7"
